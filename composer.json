{
  "name": "railroad/points",
  "type": "library",
  "description": "A user point tracking system, which is commonly used for tracking user XP, like in a video game.",
  "keywords": [
    "event",
    "data",
    "synchronizer",
    "sync"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Caleb Favor",
      "email": "calebfavor@gmail.com",
      "role": "Developer"
    }
  ],
  "prefer-stable": true,
  "require": {
    "php": "~7",
    "laravel/framework": "5.6.*",
    "railroad/resora": "^1.0",
    "railroad/permissions": "^1.0",
    "railroad/response": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~7.0",
    "orchestra/testbench": "~3.6",
    "filp/whoops":"~2.1"
  },
  "autoload": {
    "psr-4": {
      "Railroad\\Points\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Railroad\\Points\\Tests\\": "tests/"
    }
  }
}