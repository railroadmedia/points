{
  "name": "railroad/points",
  "type": "library",
  "description": "A user point tracking system, which is commonly used for tracking user XP, like in a video game.",
  "keywords": [
    "event",
    "data",
    "synchronizer",
    "sync"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Caleb Favor",
      "email": "calebfavor@gmail.com",
      "role": "Developer"
    }
  ],
  "prefer-stable": true,
  "require": {
    "php": "^8.1",
    "laravel/framework": "^9.0",
    "railroad/resora": "^2.0",
    "railroad/permissions": "^2.0",
    "railroad/response": "2.0.x-dev"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5.10",
    "phpunit/php-code-coverage": "^9.2.10",
    "orchestra/testbench": "^7.4"
  },
  "autoload": {
    "psr-4": {
      "Railroad\\Points\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Railroad\\Points\\Tests\\": "tests/"
    }
  }
}
